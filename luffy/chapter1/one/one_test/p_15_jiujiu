
# 15、利用for循环和range输出9 * 9乘法表
print("<<九九乘法表>>")
for i in range(1,10):
    for j in range(1,i+1):
        #print(f"{i}*{j} = {(i*j)}",end='  ')
        print('%d * %d = %2d' %(i,j,i*j),end='  ')
    print()


print("<<九九乘法表>>")
for i in range(1,10):
    for x in range(1,i+1):
        print( '%d X %d = %2d' % (i ,x ,i*x) ,end = '  ' )
    print('  ')
#